{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-alert": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off"
  }
}
